    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@zip.js/zip.js": "^2.3.23",
    "bootstrap": "^5.1.3",
    "jest-extended": "^1.2.0",
    "npm-run-all": "^4.1.5",
